{
    "name": "cltv/yii3sentry",
    "type": "library",
    "keywords": [
        "yii",
        "sentry"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Kirill",
            "email": "k.kikimov@webvork.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Webvork\\Yii3\\Sentry\\": "src"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.4",
        "psr/container": "^1.1",
        "psr/http-server-middleware": "^1.0",
        "psr/log": "^1.1",
        "sentry/sdk": "^3.2",
        "symfony/console": "^5.3",
        "yiisoft/log": "^1.0",
        "yiisoft/middleware-dispatcher": "^2.0",
        "yiisoft/router": "^1.0",
        "yiisoft/router-fastroute": "^1.0",
        "yiisoft/yii-console": "^1.0",
        "yiisoft/yii-http": "^1.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.6",
        "vimeo/psalm": "^4.23"
    },
    "extra": {
        "config-plugin-options": {
            "source-directory": "config"
        },
        "config-plugin": {
            "params": "params.php",
            "common": "factory.php",
            "events-console": "events-console.php",
            "events-web": "events.php",
            "bootstrap": "bootstrap.php"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "infection/extension-installer": true,
            "composer/package-versions-deprecated": true
        }
    }
}
